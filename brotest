-- Special islands use portals
local function TeleportSpecialIslandSea3(name)
    if name == "Castle On The Sea" then
        CommF_:InvokeServer("requestEntrance", Vector3.new(-5075.50927734375, 314.5155029296875, -3150.0224609375))
    elseif name == "Hydra Island" then
        CommF_:InvokeServer("requestEntrance", Vector3.new(5661.5322265625, 1013.0907592773438, -334.9649963378906))
    elseif name == "Mansion" then
        CommF_:InvokeServer("requestEntrance", Vector3.new(-12468.5380859375, 375.0094299316406, -7554.62548828125))
    end
end

-- Tween function
function Tween(destination)
    local tweenService = game:GetService("TweenService")
    local player = game.Players.LocalPlayer
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end

    local hrp = character.HumanoidRootPart
    local distance = (hrp.Position - destination.Position).Magnitude
    local tweenInfo = TweenInfo.new(distance / 350, Enum.EasingStyle.Linear)

    local tween = tweenService:Create(hrp, tweenInfo, { CFrame = destination })
    tween:Play()
    tween.Completed:Wait()
end

-- UI Section: Sea 3 Teleport
Teleport:AddSection("Sea 3")

Teleport:AddDropdown({
    Name = "Select Island Teleport",
    Flag = "Teleport/IslandSelectionSea3",
    Default = "Port Town",
    Options = {
        "Ussop Island", "Mini Sky Island", "Great Tree", "Castle On The Sea",
        "MiniSky", "Port Town", "Hydra Island", "Floating Turtle", "Mansion",
        "Haunted Castle", "Ice Cream Island", "Peanut Island", "Cake Island",
        "Cocoa Island", "Candy Island", "Tiki Outpost"
    },
    Callback = function(option)
        _G.SelectedTeleportIslandSea3 = option
        print("Selected Island (Sea 3):", option)
    end
})

Teleport:AddToggle({
    Name = "Teleport To Island",
    Flag = "Teleport/EnableTeleportSea3",
    Default = false,
    Callback = function(Value)
        if Value and game.PlaceId == 7449423635 and _G.SelectedTeleportIslandSea3 then
            local selected = _G.SelectedTeleportIslandSea3

            if IslandCFramesSea3[selected] then
                Tween(IslandCFramesSea3[selected])
            else
                TeleportSpecialIslandSea3(selected)
            end
        end
    end
})




