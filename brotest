    ["WindMill"] = CFrame.new(979.79, 16.51, 1429.04),
    ["Marine"] = CFrame.new(-2566.42, 6.85, 2045.25),
    ["Middle Town"] = CFrame.new(-690.33, 15.09, 1582.23),
    ["Jungle"] = CFrame.new(-1612.79, 36.85, 149.12),
