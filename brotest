local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/newredz/RedzLibV5/main/Source.lua"))()

-- Wait until the library is fully loaded
repeat task.wait() until redzlib

-- Create the window
local Window = redzlib:MakeWindow({
    Title = "Nova.GG : Blox-Fruit",
    SubTitle = "by Viper x Tanny",
    SaveFolder = "redz Hub | Blox Fruits.lua"
})

local Main = Window:MakeTab({
    Title = "Main Farm",
    Description = "Info",
    Icon = "home",  -- Fixed the split "Icon"
})

local Player = Window:MakeTab({
    Title = "Player Combat",
    Description = "Info",
    Icon = "swords",  -- Fixed the split "Icon"
})

local Raids = Window:MakeTab({
    Title = "Raids & Fruit",
    Description = "Info",
    Icon = "cherry",  -- Fixed the split "Icon"
})

local Teleport = Window:MakeTab({
    Title = "Teleport Sea",
    Description = "Info",
    Icon = "book",  -- Fixed the split "Icon"
})


Main:AddToggle({
    Name = "Auto Farm Level",
    Flag = "Main/AutoFarmLevel",
    Description = "Fast Farm With Dodge Skill Mobs",
    Default = false,
    Callback = function(Value)
        _G.AutoLevel = Value
        if not Value then
            wait()
            Tween(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
            wait()
        end
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoLevel then
            pcall(function()
                CheckLevel()

                local player = game:GetService("Players").LocalPlayer
                local questGui = player.PlayerGui.Main.Quest

                if not string.find(questGui.Container.QuestTitle.Title.Text, NameMon) or not questGui.Visible then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                    Tween(CFrameQ)

                    if (CFrameQ.Position - player.Character.HumanoidRootPart.Position).Magnitude <= 5 then
                        wait(0.5) -- Small delay before accepting quest
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, QuestLv)
                    end
                else
                    -- Check if there are mobs to farm
                    local foundMob = false
                    
                    for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                            if string.find(enemy.Name, Ms) then
                                foundMob = true
                                
                                -- Target and attack the mob
                                AutoHaki()
                                EquipTool(SelectWeapon)
                                
                                enemy.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                                enemy.HumanoidRootPart.Transparency = 1
                                enemy.Humanoid.JumpPower = 0
                                enemy.Humanoid.WalkSpeed = 0
                                enemy.HumanoidRootPart.CanCollide = false
                                
                                local targetPos = enemy.HumanoidRootPart.Position + Vector3.new(0, 30, 0)
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
                                
                                -- Attack
                                AttackNoCoolDown()
                                bringmob = true
                                
                                wait(0.1)
                                break -- Focus on one mob at a time
                            end
                        end
                    end
                    
                    -- If no mobs found, go to spawn location
                    if not foundMob then
                        bringmob = false
                        -- Go to mob spawn location
                        Tween(CFrameMon)
                        wait(0.5)
                    end
                end
            end)
        end
    end
end)

-- AUTO FARM NEAREST MOB TOGGLE
Main:AddToggle({
    Name = "Auto Farm Nearest Mob",
    Flag = "Main/AutoFarmNearestMob",
    Description = "Nearest",
    Default = false,
    Callback = function(Value)
        _G.AutoNear = Value
        if not Value then 
            bringmob = false
            -- Return to original position when turning off
            wait()
            Tween(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
            wait()
        end
    end
})

-- AUTO FARM NEAREST MOB LOOP
spawn(function()
    while task.wait() do
        if _G.AutoNear then
            pcall(function()
                local Closest = nil
                local ClosestDistance = math.huge
                
                -- Find the closest enemy
                for _, enemy in pairs(workspace.Enemies:GetChildren()) do
                    if enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                        local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - enemy.HumanoidRootPart.Position).Magnitude
                        if dist < ClosestDistance then
                            Closest = enemy
                            ClosestDistance = dist
                        end
                    end
                end

                if Closest then
                    -- Target and attack the closest mob
                    AutoHaki()
                    EquipTool(SelectWeapon)
                    
                    -- Modify mob properties for easier farming
                    if Closest:FindFirstChild("HumanoidRootPart") then
                        Closest.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                        Closest.HumanoidRootPart.Transparency = 1
                        Closest.HumanoidRootPart.CanCollide = false
                    end
                    if Closest:FindFirstChild("Humanoid") then
                        Closest.Humanoid.WalkSpeed = 0
                        Closest.Humanoid.JumpPower = 0
                    end
                    
                    -- Position above the mob
                    local targetPos = Closest.HumanoidRootPart.Position + Vector3.new(0, 30, 0)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
                    
                    -- Attack
                    AttackNoCoolDown()
                    bringmob = true
                    
                    -- Wait before next loop iteration
                    wait(_G.Fast_Delay or 0.1)
                    
                    -- Check if mob is dead or no longer valid
                    if not Closest or not Closest:FindFirstChild("Humanoid") or Closest.Humanoid.Health <= 0 or not Closest.Parent then
                        bringmob = false
                        wait(0.5) -- Short wait before targeting next mob
                    end
                else
                    -- No enemies found
                    bringmob = false
                    wait(0.5)
                end
            end)
        end
    end
end)

-- AUTO COLLECT CHEST TOGGLE
Main:AddSection("Chest Farm")

Main:AddToggle({
    Name = "Auto Farm Chest",
    Flag = "Main/AutoCollectChest",
    Description = "Automatically farm nearest chest",
    Default = false,
    Callback = function(Value)
        _G.AutoCollectChest = Value
        if not Value then
            wait()
            Tween(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
            wait()
        end
    end
})

-- AUTO FARM CHEST LOOP
spawn(function()
    while task.wait() do
        if _G.AutoCollectChest then
            pcall(function()
                local player = game.Players.LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()
                local root = character:FindFirstChild("HumanoidRootPart")
                if not root then return end

                local closestChest = nil
                local shortestDistance = math.huge
                local chests = game:GetService("CollectionService"):GetTagged("_ChestTagged")

                -- Find closest enabled chest
                for _, chest in pairs(chests) do
                    if chest:IsA("Model") and not chest:GetAttribute("IsDisabled") then
                        local dist = (root.Position - chest:GetPivot().Position).Magnitude
                        if dist < shortestDistance then
                            shortestDistance = dist
                            closestChest = chest
                        end
                    end
                end

                -- Go to chest
                if closestChest then
                    local targetPos = closestChest:GetPivot().Position + Vector3.new(0, 5, 0)
                    local tweenService = game:GetService("TweenService")
                    local tweenInfo = TweenInfo.new((root.Position - targetPos).Magnitude / 350, Enum.EasingStyle.Linear)
                    local goal = { CFrame = CFrame.new(targetPos) }

                    local tween = tweenService:Create(root, tweenInfo, goal)
                    tween:Play()
                    tween.Completed:Wait()
                end
            end)
        end
    end
end)

Main:AddSection("Bones Farm");

Main:AddToggle({
    Name = "Auto Farm Bones",
    Flag = "Main/AutoFarmBones",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.AutoFarmBones = Value
        if not Value then bringmob = false end
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoFarmBones then
            pcall(function()
                -- Bone Farming Setup
                Ms = "Reborn Skeleton"
                NameMon = "Reborn Skeleton"
                NameQuest = "HauntedQuest1"
                QuestLv = 1
                CFrameQ = CFrame.new(-9480.80762, 142.130661, 5566.37305, -0.00655503059, 4.5295423e-8, -0.999978542, 2.0492047e-8, 1, 4.5162068e-8, 0.999978542, -2.0195568e-8, -0.00655503059)
                CFrameMon = CFrame.new(-8761.77148, 183.431747, 6168.33301, 0.978073597, -0.000013950732, -0.208259016, -0.0000010807393, 1, -0.00007206303, 0.208259016, 0.00007070804, 0.978073597)

                local player = game.Players.LocalPlayer
                local questGui = player.PlayerGui.Main.Quest

                if not questGui.Visible or not string.find(questGui.Container.QuestTitle.Title.Text, NameMon) then
                    bringmob = false
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                    local questTween = Tween(CFrameQ)
                    if questTween then questTween.Completed:Wait() end
                    task.wait(1)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, QuestLv)
                    task.wait(0.5)
                end

                local found = false
                for _, enemy in pairs(workspace.Enemies:GetChildren()) do
                    if enemy.Name == Ms and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                        found = true
                        break
                    end
                end

                if not found then
                    Tween(CFrameMon)
                    task.wait(1)
                end

                for _, enemy in pairs(workspace.Enemies:GetChildren()) do
                    if _G.AutoFarmBones and enemy.Name == Ms and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
                        bringmob = true
                        repeat
                            if not _G.AutoFarmBones or not enemy.Parent then break end
                            AutoHaki()
                            EquipTool(SelectWeapon)
                            Tween(CFrame.new(enemy.HumanoidRootPart.Position + Vector3.new(0, 20, 0)))
                            AttackNoCoolDown()
                            if enemy:FindFirstChild("HumanoidRootPart") then
                                enemy.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                                enemy.HumanoidRootPart.Transparency = 1
                                enemy.HumanoidRootPart.CanCollide = false
                            end
                            if enemy:FindFirstChild("Humanoid") then
                                enemy.Humanoid.WalkSpeed = 0
                                enemy.Humanoid.JumpPower = 0
                            end
                            task.wait()
                        until not _G.AutoFarmBones or not enemy or not enemy.Parent or enemy.Humanoid.Health <= 0
                        bringmob = false
                    end
                end
            end)
        end
    end
end)


Main:AddToggle({
    Name = "Auto Roll Bones",
    Flag = "Main/AutoRollBones",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.AutoRollBones = Value
        if Value then
            spawn(function()
                while _G.AutoRollBones do
                    pcall(function()
                        local args = {
                            [1] = "Bones",
                            [2] = "Buy",
                            [3] = 1,
                            [4] = 1
                        }
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                    end)
                    wait(1)  -- Adjust the wait time if needed
                end
            end)
        end
    end
})


