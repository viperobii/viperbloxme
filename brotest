-- RequestEntrance Islands
local function TeleportSpecialIsland(name)
    if name == "Sky Island 2" then
        CommF_:InvokeServer("requestEntrance", Vector3.new(-4607.82, 872.54, -1667.55))
    elseif name == "Sky Island 3" then
        CommF_:InvokeServer("requestEntrance", Vector3.new(-7894.61, 5547.14, -380.29))
    elseif name == "Underwater Island" then
        CommF_:InvokeServer("requestEntrance", Vector3.new(61163.85, 11.67, 1819.78))
    end
end

-- Dropdown
Teleport:AddDropdown({
    Name = "Select Island Teleport",
    Flag = "Teleport/IslandSelection",
    Default = "WindMill",
    Options = {
        "WindMill", "Marine", "Middle Town", "Jungle", "Pirate Village", "Desert", "Snow Island",
        "Marine Ford", "Colosseum", "Sky Island 1", "Sky Island 2", "Sky Island 3",
        "Prison", "Magma Village", "Underwater Island", "Fountain City"
    },
    Callback = function(option)
        _G.SelectedTeleportIsland = option
        print("Selected Island:", _G.SelectedTeleportIsland)
    end
})

-- Toggle
Teleport:AddToggle({
    Name = "Teleport To Island",
    Flag = "Teleport/EnableTeleport",
    Default = false,
    Callback = function(Value)
        if Value and game.PlaceId == 2753915549 then -- First Sea
            if IslandCFrames[_G.SelectedTeleportIsland] then
                Tween2(IslandCFrames[_G.SelectedTeleportIsland])
            else
                TeleportSpecialIsland(_G.SelectedTeleportIsland)
            end
        end
    end
})



