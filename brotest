    Default = "Flame",
    Options = {
        "Ice", "Quake", "Light", "Dark", "Spider", "Rumble", "Magma", "Buddha", "Sand", "Phoenix", "Dough"
    },
    Callback = function(option)
        SelectChip = option
    end
})


Raids:AddToggle({
    Name = "Auto Buy Chip",
    Flag = "Raids/AutoBuyChip",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.Auto_Buy_Chips_Dungeon = Value
    end
})

spawn(function()
    while task.wait() do
        if _G.Auto_Buy_Chips_Dungeon then
            pcall(function()
                local args = {
                    [1] = "RaidsNpc",
                    [2] = "Select",
                    [3] = SelectChip
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end)
        end
    end
end)

Raids:AddToggle({
    Name = "Auto Start Raids",
    Flag = "Raids/AutoStartRaid",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.Auto_StartRaid = Value
    end
})

spawn(function()
    while wait(1) do -- Slower check to reduce lag
        if _G.Auto_StartRaid then
            pcall(function()
                local player = game.Players.LocalPlayer
                local backpack = player.Backpack
                local char = player.Character
                
                -- Check for chip in both backpack and character
                local hasChip = backpack:FindFirstChild("Special Microchip") or (char and char:FindFirstChild("Special Microchip"))
                
                -- Check if raid is already in progress
                local timerVisible = player.PlayerGui.Main.Timer.Visible
                local hasIsland = workspace["_WorldOrigin"].Locations:FindFirstChild("Island 1")
                
                if not timerVisible and not hasIsland then
                    if hasChip then
                        -- Set spawn point first
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
                        
                        -- Different handling based on sea level
                        if Sea2 then
                            -- Teleport to Second Sea raid location
                            Tween2(CFrame.new(-6438.73535, 250.645355, -4501.50684))
                            wait(1) -- Wait for teleport to complete
                            
                            -- Make sure we're close to the button
                            local button = workspace.Map.CircleIsland.RaidSummon2.Button
                            if button and button:FindFirstChild("Main") and button.Main:FindFirstChild("ClickDetector") then
                                local distance = (button.Main.Position - player.Character.HumanoidRootPart.Position).Magnitude
                                if distance <= 10 then
                                    fireclickdetector(button.Main.ClickDetector)
                                    print("Clicked Second Sea raid button")
                                else
                                    -- Try to teleport closer
                                    Tween2(button.Main.CFrame)
                                    wait(1)
                                    fireclickdetector(button.Main.ClickDetector)
                                    print("Clicked Second Sea raid button after repositioning")
                                end
                            end
                        elseif Sea3 then
                            -- Request entrance to Third Sea raid location
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(-5075.509, 314.5155, -3150.022))
                            wait(1) -- Wait for teleport to complete
                            
                            -- Teleport to the button
                            Tween2(CFrame.new(-5017.40869, 314.844055, -2823.0127))
                            wait(1) -- Wait for teleport to complete
                            
                            -- Make sure we're close to the button
                            local button = workspace.Map["Boat Castle"].RaidSummon2.Button
                            if button and button:FindFirstChild("Main") and button.Main:FindFirstChild("ClickDetector") then
                                local distance = (button.Main.Position - player.Character.HumanoidRootPart.Position).Magnitude
                                if distance <= 10 then
                                    fireclickdetector(button.Main.ClickDetector)
                                    print("Clicked Third Sea raid button")
                                else
                                    -- Try to teleport closer
                                    Tween2(button.Main.CFrame)
                                    wait(1)
                                    fireclickdetector(button.Main.ClickDetector)
                                    print("Clicked Third Sea raid button after repositioning")
                                end
                            end
                        end
                    else
                        print("No Special Microchip found!")
                    end
                end
            end)
        end
    end
end)

Raids:AddToggle({
    Name = "Auto Raids Mobs",
    Flag = "Raids/AutoFarmNearestMob",
    Description = "Nearest",
    Default = false,
    Callback = function(Value)
        _G.AutoNear = Value
        if not Value then 
            bringmob = false
            -- Return to original position when turning off
            wait()
            Tween(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
            wait()
        end
    end
})

-- AUTO FARM NEAREST MOB LOOP
spawn(function()
    while task.wait() do
        if _G.AutoNear then
            pcall(function()
                local Closest = nil
                local ClosestDistance = math.huge
                
                -- Find the closest enemy
                for _, enemy in pairs(workspace.Enemies:GetChildren()) do
                    if enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                        local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - enemy.HumanoidRootPart.Position).Magnitude
                        if dist < ClosestDistance then
                            Closest = enemy
                            ClosestDistance = dist
                        end
                    end
                end

                if Closest then
                    -- Target and attack the closest mob
                    AutoHaki()
                    EquipTool(SelectWeapon)
                    
                    -- Modify mob properties for easier farming
                    if Closest:FindFirstChild("HumanoidRootPart") then
                        Closest.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                        Closest.HumanoidRootPart.Transparency = 1
                        Closest.HumanoidRootPart.CanCollide = false
                    end
                    if Closest:FindFirstChild("Humanoid") then
                        Closest.Humanoid.WalkSpeed = 0
                        Closest.Humanoid.JumpPower = 0
                    end
                    
                    -- Position above the mob
                    local targetPos = Closest.HumanoidRootPart.Position + Vector3.new(0, 30, 0)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
                    
                    -- Attack
                    AttackNoCoolDown()
                    bringmob = true
                    
                    -- Wait before next loop iteration
                    wait(_G.Fast_Delay or 0.1)
                    
                    -- Check if mob is dead or no longer valid
                    if not Closest or not Closest:FindFirstChild("Humanoid") or Closest.Humanoid.Health <= 0 or not Closest.Parent then
                        bringmob = false
                        wait(0.5) -- Short wait before targeting next mob
                    end
                else
                    -- No enemies found
                    bringmob = false
                    wait(0.5)
                end
            end)
        end
    end
end)


Raids:AddToggle({
    Name = "Auto Next Island",
    Flag = "Raids/AutoNextIsland",
    Description = "",
    Default = false,
    Callback = function(Value)
        AutoNextIsland = Value
        if not Value then
            _G.AutoNear = false
        end
    end
})

spawn(function()
    local VisitedIslands = {}
    while task.wait() do
        if AutoNextIsland then
            pcall(function()
                local char = game.Players.LocalPlayer.Character
                if char and char:FindFirstChild("HumanoidRootPart") then
                    local hrp = char.HumanoidRootPart
                    local pos = hrp.Position
                    local Locations = workspace["_WorldOrigin"].Locations

                    -- Reset tracker if teleported back to raid area
                    if (pos - Vector3.new(-6438.735, 250.645, -4501.506)).Magnitude < 1 or
                       (pos - Vector3.new(-5017.408, 314.844, -2823.012)).Magnitude < 1 then
                        VisitedIslands = {}
                    end

                    if Locations:FindFirstChild("Island 1") then
                        _G.AutoNear = true
                    end

                    for i = 2, 5 do
                        local islandName = "Island " .. i
                        if Locations:FindFirstChild(islandName) and not VisitedIslands[islandName] then
                            Tween(Locations[islandName].CFrame)
                            VisitedIslands[islandName] = true
                            AutoNextIsland = false
                            wait()
                            AutoNextIsland = true
                            break
                        end
                    end
                end
            end)
        end
    end
end)


local v22 = Instance.new("ScreenGui")
local v23 = Instance.new("ImageButton")
local v24 = Instance.new("UICorner")
local v25 = Instance.new("ParticleEmitter")
local v26 = game:GetService("TweenService")

v22.Parent = game.CoreGui
v22.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

v23.Parent = v22
v23.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
v23.BorderSizePixel = 0
v23.Position = UDim2.new(0.1, 0, 0.1, 0)  -- Position of the floating button
v23.Size = UDim2.new(0, 50, 0, 50)  -- Size of the floating logo
v23.Draggable = true  -- Makes it draggable
v23.Image = "rbxassetid://101883839381557"  -- Fixed image ID (removed extra digits)

v24.Parent = v23
v24.CornerRadius = UDim.new(1, 0)  -- Round corners

v25.Parent = v23
v25.LightEmission = 1
v25.Size = NumberSequence.new({
    NumberSequenceKeypoint.new(0, 0.1),
    NumberSequenceKeypoint.new(1, 0)
})
v25.Lifetime = NumberRange.new(0.5, 1)
v25.Rate = 0
v25.Speed = NumberRange.new(5, 10)
v25.Color = ColorSequence.new(Color3.fromRGB(255, 85, 255), Color3.fromRGB(85, 255, 255))  -- Particle colors

-- Animation on button press
v23.MouseButton1Down:Connect(function()
    v25.Rate = 100  -- Start emitting particles
    
    -- Toggle UI visibility
    if Window.Enabled then
        Window:Minimize(true)
    else
        Window:Minimize(false)
    end
    
    task.delay(1, function()
        v25.Rate = 0  -- Stop emitting after 1 second
    end)
end)

-- Optional Tween effect for smooth appearance
local v47 = v26:Create(v23, TweenInfo.new(0.5, Enum.EasingStyle.Bounce, Enum.EasingDirection.Out), {
    Position = UDim2.new(0.1, 0, 0.1, 0),
    Size = UDim2.new(0, 50, 0, 50),
    BackgroundTransparency = 0.2
})
v47:Play()




