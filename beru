Main:AddSection("Farm")

Main:AddToggle({
    Name = "Auto Farm Level",
    Flag = "Main/AutoFarmLevel",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.AutoLevel = Value
        if not Value then
            wait()
            local player = game:GetService("Players").LocalPlayer
            local pos = player.Character.HumanoidRootPart.Position
            if pos.Y <= 0 then
                player.Character.HumanoidRootPart.CFrame = CFrame.new(pos.X, 5, pos.Z)
            end
            Tween(player.Character.HumanoidRootPart.CFrame)
            wait()
        end
    end
})

spawn(function()
    while task.wait() do
        if _G.AutoLevel then
            pcall(function()
                CheckLevel()

                local player = game:GetService("Players").LocalPlayer
                local questGui = player.PlayerGui.Main.Quest

                if not string.find(questGui.Container.QuestTitle.Title.Text, NameMon) or not questGui.Visible then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                    Tween(CFrameQ)

                    local pos = player.Character.HumanoidRootPart.Position
                    if pos.Y <= 0 then
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(pos.X, 5, pos.Z)
                    end

                    if (CFrameQ.Position - pos).Magnitude <= 5 then
                        wait(0.5)
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, QuestLv)
                    end
                else
                    local foundMob = false
                    for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if enemy:FindFirstChild("Humanoid") and enemy:FindFirstChild("HumanoidRootPart") and enemy.Humanoid.Health > 0 then
                            if string.find(enemy.Name, Ms) then
                                foundMob = true

                                AutoHaki()
                                EquipTool(SelectWeapon)

                                enemy.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                                enemy.HumanoidRootPart.Transparency = 1
                                enemy.Humanoid.JumpPower = 0
                                enemy.Humanoid.WalkSpeed = 0
                                enemy.HumanoidRootPart.CanCollide = false

                                local targetPos = enemy.HumanoidRootPart.Position + Vector3.new(0, 30, 0)
                                player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)

                                AttackNoCoolDown()
                                bringmob = true

                                wait(0.1)
                                break
                            end
                        end
                    end

                    if not foundMob then
                        bringmob = false
                        Tween(CFrameMon)

                        local pos = player.Character.HumanoidRootPart.Position
                        if pos.Y <= 0 then
                            player.Character.HumanoidRootPart.CFrame = CFrame.new(pos.X, 5, pos.Z)
                        end

                        wait(0.5)
                    end
                end
            end)
        end
    end
end)

Player:AddSection("Anti Slow");

-- Speed Multiplier Slider
Player:AddSlider({
    Name = "Walkspeed",
    Flag = "Player/SpeedMultiplier",
    Description = "Make It Low To Increase Speed Lol",
    Range = {1, 10},
    Suffix = "×",
    CurrentValue = 2,
    Increment = 0.1,
    Callback = function(Value)
        _G.SpeedMultiplier = Value
    end,
})

-- Initialize global variables
_G.SpeedBoost = false
_G.SpeedMultiplier = 2

-- Loop for velocity-based speed modification
task.spawn(function()
    while true do
        if _G.SpeedBoost then
            pcall(function()
                local char = game.Players.LocalPlayer.Character
                if char and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChild("Humanoid") then
                    -- Get current movement direction
                    local humanoid = char.Humanoid
                    local moveDirection = humanoid.MoveDirection
                    
                    -- Only apply if player is actually moving
                    if moveDirection.Magnitude > 0 then
                        -- Normalize and apply multiplier
                        local velocity = moveDirection.Unit * (_G.SpeedMultiplier * 30)
                        -- Preserve Y velocity to maintain jumping
                        velocity = Vector3.new(velocity.X, char.HumanoidRootPart.Velocity.Y, velocity.Z)
                        -- Apply velocity
                        char.HumanoidRootPart.Velocity = velocity
                    end
                end
            end)
        end
        task.wait()
    end
end)

Player:AddToggle({
    Name = "Enable Walkspeed",
    Flag = "Player/SpeedBoost",
    Description = "",
    Default = false,
    Callback = function(Value)
        _G.SpeedBoost = Value
        if not Value then
            -- Reset any active speed modifications when disabled
            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                char.HumanoidRootPart.Velocity = Vector3.new(0, char.HumanoidRootPart.Velocity.Y, 0)
            end
        end
    end,
})








