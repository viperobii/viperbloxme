    local v852 = v838(workspace, "_WorldOrigin");
    local v853 = v838(workspace, "Characters");
    local v854 = v838(workspace, "Enemies");
    local v855 = v838(workspace, "Map");
    local v856 = v838(v852, "EnemySpawns");
    local v857 = v838(v852, "Locations");
