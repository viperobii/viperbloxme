                    else
                        v809.Handle["NameEsp" .. Number ].TextLabel.Text = v809.Name .. "   \n" .. v45((game:GetService("Players").LocalPlayer.Character.Head.Position - v809.Handle.Position).Magnitude / 3) .. " Distance" ;
